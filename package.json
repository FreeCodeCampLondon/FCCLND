{
  "name": "fcclnd-app",
  "version": "1.0.0",
  "description": "Portal for FreeCodeCamp London Campers",
  "contributors": [
    "Alex Garulli",
    "Chris Alexander",
    "Danny Lewis"
  ],
  "main": "app.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --inline --hot",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "serve": "npm run build && ./node_modules/.bin/browser-sync start -s './dist'",
    "gh-pages": "./node_modules/.bin/gh-pages -d ./dist",
    "deploy": "npm run build && npm run gh-pages",
    "deploy:prod": "npm run build && ./node_modules/.bin/gh-pages -o upstream -d ./dist",
    "lint": "NODE_ENV=development ./node_modules/.bin/eslint dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FreeCodeCampLondon/FCCLND.git"
  },
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/FreeCodeCampLondon/FCCLND/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.13.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-config-xo-react": "^0.9.0",
    "eslint-config-xo-space": "^0.14.0",
    "eslint-loader": "^1.4.1",
