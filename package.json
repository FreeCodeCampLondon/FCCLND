{
  "name": "fcclnd-app",
  "version": "2.0.0",
  "description": "Portal for FreeCodeCamp London Campers",
  "private": true,
  "contributors": [
    "Alex Garulli",
    "Chris Alexander",
    "Danny Lewis"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-sass": "node-sass src/ -o src/",
    "watch-sass": "npm run build-sass && node-sass src/ -o src/ --watch --recursive",
    "serve": "npm run build && ./node_modules/.bin/browser-sync start -s './dist'",
    "gh-pages": "./node_modules/.bin/gh-pages -d ./dist",
    "deploy": "npm run build && npm run gh-pages",
    "deploy:prod": "npm run build && ./node_modules/.bin/gh-pages -o upstream -d ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FreeCodeCampLondon/FCCLND.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FreeCodeCampLondon/FCCLND/issues"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.17.0",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "gh-pages": "^0.11.0",
    "node-sass": "^4.5.0",
    "react-scripts": "^0.6.1"
  },
  "dependencies": {
    "bootstrap": "3",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
